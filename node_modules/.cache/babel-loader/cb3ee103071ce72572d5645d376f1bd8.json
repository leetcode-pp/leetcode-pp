AA;AACA,KAHA;AAIA,IAAA,oBAJA,kCAIA;AACA,WAAA,SAAA,CAAA;AAAA,eACA,QAAA,CAAA,gBAAA,CAAA,MAAA,EAAA,OAAA,CAAA,IAAA,CAAA,cAAA,CADA;AAAA,OAAA;AAGA,KARA;AASA,IAAA,cATA,gCASA;AAAA,UAAA,EAAA,QAAA,EAAA;AAAA,UAAA,IAAA,QAAA,IAAA;AACA,WAAA,gBAAA,GAAA,EAAA;AACA,WAAA,mBAAA,GAAA,IAAA;AACA,WAAA,oBAAA;AACA;AAbA,GAjBA;AAgCA,EAAA,KAAA,EAAA;AACA,IAAA,gBADA,4BACA,EADA,EACA;AACA,WAAA,KAAA,GAAA,KAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OAAA,KAAA,EAAA;AACA;AAHA;AAhCA,CAAA","sourcesContent":["<template>\n  <div class=\"max-width-600-center\">\n    <form>\n      <div class=\"drown-wrapper\">\n        <a-dropdown :trigger=\"['click']\">\n          <a class=\"ant-dropdown-link\" href=\"#\">\n            {{ selctedChapterLabel }} <a-icon type=\"caret-down\" />\n          </a>\n          <a-menu slot=\"overlay\">\n            <a-menu-item v-for=\"(block, index) in blocks\" :key=\"index\">\n              <a @click=\"handleCHchange(block)\">{{ block.name }}</a>\n            </a-menu-item>\n          </a-menu>\n        </a-dropdown>\n      </div>\n    </form>\n    <div v-bind:key=\"item.title\" v-for=\"item in block.list\">\n      <div class=\"code-block-title\">{{ item.title }}</div>\n      <a-tabs\n        @change=\"handleLanguageChange\"\n        tabPosition=\"left\"\n        v-if=\"selctedChapterId\"\n      >\n        <a-tab-pane\n          :tab=\"code.language\"\n          v-bind:key=\"code.language\"\n          v-for=\"code in item.codes\"\n          ><div class=\"text-align-left\" v-html=\"render(code)\"></div\n        ></a-tab-pane>\n      </a-tabs>\n    </div>\n  </div>\n</template>\n\n<script>\nimport hljs from 'highlight.js'\nimport MarkdownIt from 'markdown-it'\nimport 'highlight.js/styles/github.css'\n// import copy from 'markdown-it-code-copy'\n\nconst md = new MarkdownIt()\n// md.use(copy, {\n//   buttonStyle:\n//     'position: absolute; top: 7.5px; right: 6px; cursor: pointer; border: none;'\n// })\n\nfunction render(s) {\n  return md.render(s)\n}\n\nexport default {\n  data() {\n    return {\n      block: {},\n      selctedChapterId: '',\n      selctedChapterLabel: '请选择章节',\n      blocks: []\n    }\n  },\n  created() {\n    import('./Book.js').then(m => {\n      this.blocks = m.default\n      this.selctedChapterId = '01'\n      this.selctedChapterLabel = '第一章'\n      this.handleLanguageChange()\n    })\n  },\n  methods: {\n    render(code) {\n      return render('```' + code.language + '\\n' + code.content + '```')\n    },\n    handleLanguageChange() {\n      this.$nextTick(() =>\n        document.querySelectorAll('code').forEach(hljs.highlightBlock)\n      )\n    },\n    handleCHchange({ id, name }) {\n      this.selctedChapterId = id\n      this.selctedChapterLabel = name\n      this.handleLanguageChange()\n    }\n  },\n  watch: {\n    selctedChapterId(id) {\n      this.block = this.blocks.find(b => b.id === id) || {}\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.code-block-title {\n  font-weight: bold;\n  font-size: 18px;\n  margin: 10px 0;\n}\n.text-align-left {\n  text-align: left;\n}\n\n.drown-wrapper {\n  display: flex;\n  justify-content: end;\n  align-items: center;\n  text-align: left;\n  width: 240px;\n  margin-left: 10px;\n\n  background-color: #e6f7ff;\n  border: 1px solid #87e8de;\n  border-radius: 4px;\n  a {\n    font-size: 14px;\n    color: #1890ff;\n  }\n}\n.ant-dropdown-link {\n  width: 100%;\n  color: #fff;\n  text-align: center;\n  font-weight: 600;\n  position: relative;\n  text-decoration: none;\n  i {\n    position: absolute;\n    right: 10px;\n    top: 2px;\n  }\n}\n.max-width-600-center {\n  margin: 10px auto;\n}\n</style>\n"],"sourceRoot":"src/views"}]}