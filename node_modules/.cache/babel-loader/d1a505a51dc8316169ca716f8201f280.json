d.renderer.rules.fence = (tokens, idx, options, env, slf) => {\n    const old = oldFence(tokens, idx, options, env, slf)\n    const preReg = /<pre><code[\\w\\s-=\"]*>/\n    // 微信专属\n    if (preReg.exec(old)) {\n      const pre = preReg.exec(old)[0]\n      const post = `</code></pre>`\n      return old.replace(pre, '').replace(post, '')\n    }\n    // 自定义\n    else {\n      return old\n    }\n  }\n}\n"]}]}